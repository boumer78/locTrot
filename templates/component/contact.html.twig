
{%  extends 'layout.html.twig' %}


{% block title %}
    Formulaire de contact
{% endblock %}

{%  block stylesheets %}
    {{ parent() }}
    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Dropify/0.2.2/css/dropify.min.css" integrity="sha256-AWdeVMUYtwLH09F6ZHxNgvJI37p+te8hJuSMo44NVm0=" crossorigin="anonymous" />
{% endblock %}


{% block javascripts %}
    {{ parent() }}
{% endblock %}

{% block content %}
{% block body %}
    <header class="major narrow">
        <h2>FORMULAIRE DE CONTACT</h2>
        <p>Une question? Contactez-nous!</p>
    </header>
    <div class="container">
        <div class="row">
            <div class="col-sm-4">
                <h2 class=page-header>Contactez-nous</h2>
                *
                {{form_start(form)}}
                {{form_widget(form)}}
                {{form_end(form)}}
            </div>
        </div>
    </div>
{% endblock %}
{% endblock %}